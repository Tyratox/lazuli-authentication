{
	"plugins": [],
	"recurseDepth": 10,
	"source": {
		"includePattern": ".+\\.m?js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true
	},
	"opts": {
		"encoding": "utf8",
		"private": true,
		"recurse": true,
		"readme": "./README.md",
		"destination": "./docs/"
	}
}
